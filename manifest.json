{
  "manifest_version": 3,
  "name": "Unacademy Watermark Remover",
  "version": "1.0",
  "description": "Remove account watermark from Unacademy hosted videos",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
